{
  "name": "@rspack/plugin-preact-refresh",
  "version": "1.1.2",
  "repository": "https://github.com/rspack-contrib/rspack-plugin-preact-refresh",
  "license": "MIT",
  "description": "Preact refresh plugin for Rspack",
  "type": "module",
  "main": "index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./index.cjs"
    },
    "./intercept": "./client/reactRefresh.js",
    "./prefresh": "./client/prefresh.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint": "biome check .",
    "lint:write": "biome check . --write",
    "prepare": "simple-git-hooks && npm run build",
    "test": "cross-env RSPACK_HOT_TEST=true jest --colors",
    "testu": "cross-env RSPACK_HOT_TEST=true jest --colors --updateSnapshot",
    "release": "node ./scripts/release.mjs"
  },
  "files": [
    "client",
    "dist",
    "index.cjs"
  ],
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@prefresh/core": "^1.5.7",
    "@prefresh/utils": "^1.2.1",
    "@rslib/core": "^0.12.4",
    "@rspack/core": "1.5.1",
    "@rspack/test-tools": "1.5.1",
    "@swc/helpers": "0.5.17",
    "@swc/plugin-prefresh": "^6.3.2",
    "@types/jest": "29.5.14",
    "@types/node": "^22.18.0",
    "cross-env": "^7.0.3",
    "execa": "9.6.0",
    "fs-extra": "11.3.1",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "jest-environment-node": "29.7.0",
    "nano-staged": "^0.8.0",
    "preact": "^10.27.1",
    "semver": "7.7.2",
    "simple-git-hooks": "^2.13.1",
    "ts-jest": "29.4.1",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "@prefresh/core": "^1.5.0",
    "@prefresh/utils": "^1.2.0"
  },
  "packageManager": "pnpm@9.15.9",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  }
}
